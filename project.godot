config_version=5

[application]

config/name="First Person Shooter Prototype"
run/main_scene="res://Scenes/Levels/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=1000
config/icon="res://Assets/Textures/icon.svg"

[autoload]

global="*res://Scripts/GLOBALS/Global.gd"
SlotBase="*res://Scripts/GLOBALS/slot_base.gd"
AmmoManager="*res://Scripts/GLOBALS/ammo_manager.gd"
StatusManager="*res://Scripts/GLOBALS/status_manager.gd"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="First Person Shooter Prototype"

[file_customization]

folder_colors={
"res://Assets/": "orange",
"res://Assets/3D Models/": "orange",
"res://Assets/Sounds/": "yellow",
"res://Scenes/": "blue",
"res://Scenes/props/": "teal",
"res://Scripts/": "purple",
"res://Scripts/Player/": "pink",
"res://addons/": "gray"
}

[global_group]

Enemies="Enemies + etc"

